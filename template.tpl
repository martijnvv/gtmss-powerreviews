___SANDBOXED_JS_FOR_SERVER___

// https://apidocs.powerreviews.com/docs/content-api/affb601a1f9ac-submit-orders
const sendHttpRequest = require('sendHttpRequest');
const JSON = require('JSON');
const makeInteger = require('makeInteger');
const makeString = require('makeString');
const logToConsole = require('logToConsole');
const math = require('Math');
const getType = require('getType');

// get variables from Fields
const apiAccessToken = data.apiAccessToken;
const items = data.ecommerceItems;

if(data.logMode == "log"){
  logToConsole(items);
}  
// generate the items object for each event
const mapProductData = i => {
  const itemObj = {
    product_page_id: i.item_id,
    page_id: i.item_id,    
    product: i.item_name,
    price: i.price,
    quantity: i.quantity ? math.round(i.quantity) : i.quantity
  };
  return itemObj;
};



// Construct the user object as per Power Reviews API requirements
const event = [{
    email: data.email,
    first_name: data.firstName,
    last_name: data.lastName,
    user_id: data.userId,
    locale: data.locale,
    merchant_id: makeInteger(data.merchantId),
    merchant_group_id: makeInteger(data.merchantGroupId),  
    order_date: makeInteger(data.orderDate),
    order_id: makeString(data.orderId),
    order_items: mapProductData  
}];

// Power Reviews Enterprise API endpoint
const api_endpoint = 'https://enterprise-api.powerreviews.com/v1/orders';
const requestEndpoint = api_endpoint;
const eventRequest = event;


// Prepare request headers
const requestHeaders = {
  headers: {
    'Content-Type': 'application/json',
    'Accept': 'application/json',    
    'Authorization': apiAccessToken
  },
  method: 'POST'
};

// Send the HTTP request
sendHttpRequest(
  requestEndpoint,
  (responseStatusCode, responseHeaders, responseBody) => {
      logToConsole(JSON.stringify({
        'Name': 'Power Reviews Enterprise API',
        'Type': 'Response',
        'MerchantId': data.merchantId,
        'ResponseStatusCode': responseStatusCode,
        'ResponseHeaders': responseHeaders,
        'ResponseBody': responseBody
      }));
    if (responseStatusCode >= 200 && responseStatusCode < 300) {
      data.gtmOnSuccess();
    } else {
      data.gtmOnFailure();
    }
  },
  requestHeaders,
  JSON.stringify(eventRequest)
);


